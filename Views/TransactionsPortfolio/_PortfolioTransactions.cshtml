@model IEnumerable<StockMonitor_2.ViewModels.TransactionsPortfolio>

    <table class="table">
        <tr>
            <th>@Html.DisplayNameFor(model => model.Yritys)</th>
            <th>@Html.DisplayNameFor(model => model.Pvm)</th>
            <th>@Html.DisplayNameFor(model => model.OstoMyynti)</th>
            <th>@Html.DisplayNameFor(model => model.Maara)</th>
            <th>@Html.DisplayNameFor(model => model.aHinta)</th>
            <th>@Html.DisplayNameFor(model => model.Total)</th>
            <th>@Html.DisplayNameFor(model => model.Valuutta)</th>
            <th>@Html.DisplayNameFor(model => model.Kurssi)</th>
            <th>@Html.DisplayNameFor(model => model.TotalEuros)</th>
            <th>@Html.DisplayNameFor(model => model.Kulut)</th>
            <th>@Html.DisplayNameFor(model => model.Grandtotal)</th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Yritys)</td>
                <td>@Html.DisplayFor(modelItem => item.Pvm)</td>
                <td>@Html.DisplayFor(modelItem => item.OstoMyynti)</td>
                <td>@Html.DisplayFor(modelItem => item.Maara)</td>
                <td>@Html.DisplayFor(modelItem => item.aHinta)</td>
                <td>@Html.DisplayFor(modelItem => item.Total)</td>
                <td>@Html.DisplayFor(modelItem => item.Valuutta)</td>
                <td>@Html.DisplayFor(modelItem => item.Kurssi)</td>
                <td>@Html.DisplayFor(modelItem => item.TotalEuros)</td>
                <td>@Html.DisplayFor(modelItem => item.Kulut)</td>
                <td>@Html.DisplayFor(modelItem => item.Grandtotal)</td>
            </tr>
        }

    </table>